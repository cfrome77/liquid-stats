{
  "name": "liquid-stats",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project liquid-stats",
    "build": "ng build --project liquid-stats",
    "watch": "ng build --project liquid-stats --watch --configuration development",
    "test": "ng test --project liquid-stats",
    "lint": "ng lint --project liquid-stats",
    "prebuild": "node set-env.js --env=dev",
    "build:prod": "node set-env.js --env=prod && ng build --project liquid-stats --configuration production",
    "start:dev": "node set-env.js --env=dev && ng serve --project liquid-stats",
    "start:local": "node -r dotenv/config node set-env.js --env=dev && ng serve --project liquid-stats",
    "analyze": "webpack-bundle-analyzer dist/liquid-stats/stats.json"
  },
  "dependencies": {
    "@angular/animations": "15.2.10",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.10",
    "@angular/compiler": "15.2.10",
    "@angular/core": "15.2.10",
    "@angular/flex-layout": "15.0.0-beta.42",
    "@angular/forms": "15.2.10",
    "@angular/material": "15.2.9",
    "@angular/platform-browser": "15.2.10",
    "@angular/platform-browser-dynamic": "15.2.10",
    "@angular/router": "15.2.10",
    "chart.js": "^3.9.1",
    "dotenv": "^16.4.7",
    "leaflet": "1.7.0",
    "leaflet.markercluster": "^1.5.3",
    "ng2-charts": "^3.1.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.8.1",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.10",
    "@angular/cli": "15.2.10",
    "@angular/compiler-cli": "15.2.10",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.7.11",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/node": "^20.5.1",
    "globby": "^11.1.0",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "nodemon": "^3.1.4",
    "path-type": "^4.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}